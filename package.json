{
  "name": "silentwalker_gallery",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
  
    "optimize": "next-image-export-optimizer",
    "sync": "rclone copy -v \"drive_gallery:SiLeNtWaLkEr Destiny Emblem Wallpapers\" public/images/drive_gallery && npm run sync_git", 
    "sync_git": "git add public/images/drive_gallery/** && (git diff --quiet && git diff --staged --quiet || git commit -am 'Update images') && git push",
    "deploy": "node cleanBuild.js && touch ./out/.nojekyll && gh-pages -d out",

    "export": "next build && next export && npm run optimize",
    "update": "git pull && npm run sync && npm run export && npm run deploy"
  },
  "dependencies": {
    "bootstrap": "^5.1.3",
    "classnames": "^2.3.1",
    "cookie-cutter": "^0.2.0",
    "fs-extra": "^10.0.0",
    "gh-pages": "^4.0.0",
    "glob": "^7.2.0",
    "next": "^12.1.0",
    "next-image-export-optimizer": "^0.9.6",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "responsive-loader": "^2.3.0",
    "sass": "^1.49.7",
    "sharp": "^0.30.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/eslint-parser": "^7.17.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "12.0.10",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.28.0"
  }
}
